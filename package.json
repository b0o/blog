{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "A blog to share stuff I find interesting or helpful and experiment with web technologies.",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "prettier --check src && eslint . && astro check && tsc --noEmit",
    "fmt": "prettier --write src",
    "build": "astro build",
    "preview": "astro preview",
    "test": "playwright test",
    "generate-posts-images": "node ./scripts/generate-posts-images.js",
    "generate-devicons": "svgr --icon 1em --typescript --jsx-runtime automatic --index-template ./src/assets/svgs/devicons/index-template.js --svgo-config ./src/assets/svgs/devicons/svgo.config.js --out-dir ./src/components/devicons src/assets/svgs/devicons",
    "copy-pdf-worker": "node ./scripts/copy-pdf-worker.js",
    "postinstall": "npx patch-package && npm run generate-devicons && npm run copy-pdf-worker && npx playwright install && npx astro sync",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.2.2",
    "@astrojs/mdx": "^0.19.0",
    "@astrojs/react": "^2.1.1",
    "@astrojs/rss": "^2.3.2",
    "@astrojs/sitemap": "^1.2.2",
    "@astrojs/tailwind": "^3.1.1",
    "@codesandbox/sandpack-client": "^2.6.0",
    "@codesandbox/sandpack-react": "^2.6.1",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@floating-ui/dom": "^1.2.6",
    "@fontsource/fira-sans": "^4.5.10",
    "@fontsource/merriweather": "^4.5.14",
    "@playwright/test": "^1.32.3",
    "@svgr/cli": "^7.0.0",
    "@types/lodash": "^4.14.194",
    "@types/md5": "^2.3.2",
    "@types/mustache": "^4.2.2",
    "@types/node-fetch": "^2.6.3",
    "@types/pdfobject": "^2.2.3",
    "@types/react": "^18.0.37",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "a11y-dialog": "^7.5.2",
    "astro": "^2.4.2",
    "astro-robots-txt": "^0.3.12",
    "astro-seo": "^0.6.1",
    "astro-toc": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "eslint": "^8.38.0",
    "eslint-mdx": "^2.0.5",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "hast-util-from-html": "^1.0.1",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "husky": "^8.0.3",
    "i18next": "^22.4.15",
    "lodash": "^4.17.21",
    "lucide-react": "^0.183.0",
    "md5": "^2.3.0",
    "mdast-util-to-string": "^3.2.0",
    "node-fetch": "^2.6.9",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "patch-package": "^6.5.1",
    "pdfjs-dist": "^2.16.105",
    "pdfobject": "^2.2.8",
    "playwright": "^1.32.3",
    "postcss": "^8.4.23",
    "postcss-cli": "^8.3.1",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^14.1.0",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react": "^18.2.0",
    "react-compare-slider": "^2.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-katex": "^6.0.2",
    "rehype-slug": "^5.1.0",
    "remark-directive": "^2.0.1",
    "remark-math": "^5.1.1",
    "shiki-twoslash": "^3.1.1",
    "start-server-and-test": "^1.15.4",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "unist-util-visit": "^4.1.2"
  }
}
