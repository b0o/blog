    "remark-math": "^5.1.1",
    "shiki-twoslash": "^3.1.1",
    "start-server-and-test": "^1.14.0",
    "tailwind-merge": "^1.8.0",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.9.4",
    "unist-util-visit": "^4.1.1",
    "husky": "^8.0.3"
  }
}
