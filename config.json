{
  "DefaultContentLanguage": "en",
  "assetsDir": "assets",
  "baseURL": "https://phelipetls.github.io",
  "languageCode": "en-us",
  "pygmentsUseClasses": true,
  "summaryLength": 20,
  "title": "Phelipe Teles",
  "enableGitInfo": true,
  "build": {
    "writeStats": true,
    "noJSConfigInAssets": true
  },
  "languages": {
    "en": {
      "languageName": "English",
      "weight": 1,
      "params": {
        "resume": "https://phelipetls.github.io/latex-resume/phelipe-teles-resume-en.pdf",
        "postImagePath": "post-image.png"
      }
    },
    "pt": {
      "languageName": "Português",
      "weight": 2,
      "params": {
        "description": "Desenvolvedor web front-end que escreve sobre a prática da programação e suas ferramentas",
        "htmlLang": "pt-BR",
        "resume": "https://phelipetls.github.io/latex-resume/phelipe-teles-resume-ptbr.pdf",
        "postImagePath": "post-image.pt.png"
      }
    }
  },
  "markup": {
    "goldmark": {
      "renderer": {
        "unsafe": true
      }
    },
    "tableOfContents": {
      "ordered": true,
      "startLevel": 1
    }
  },
  "params": {
    "author": "Phelipe Teles",
    "description": "Front-end web developer that writes about the programming craft and its tools",
    "email": "phelipe_teles@hotmail.com",
    "github": "https://github.com/phelipetls",
    "linkedin": "https://linkedin.com/in/phelipeteles",
    "resume": "https://phelipetls.github.io/latex-resume/phelipe-teles-resume.pdf"
  },
  "taxonomies": {
    "tag": "tags"
  },
  "outputFormats": {
    "image": {
      "name": "image",
      "basename": "image",
      "mediaType": "text/html",
      "isHTML": "true"
    },
    "images": {
      "name": "images",
      "basename": "images",
      "mediaType": "application/json"
    }
  },
  "outputs": {
    "page": [
      "html",
      "image"
    ],
    "section": [
      "html",
      "rss",
      "images"
    ]
   }
}
