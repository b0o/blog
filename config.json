{
  "DefaultContentLanguage": "en",
  "assetsDir": "assets",
  "baseURL": "https://phelipetls.github.io",
  "languageCode": "en-us",
  "pygmentsUseClasses": true,
  "summaryLength": 20,
  "title": "Phelipe Teles",
  "enableGitInfo": true,
  "build": {
    "writeStats": true,
    "noJSConfigInAssets": true
  },
  "languages": {
    "en": {
      "languageName": "English",
      "weight": 1
    },
    "pt": {
      "languageName": "Português",
      "weight": 2,
      "params": {
        "description": "Um blog para compartilhar meus interesses em programação, estatística e mais",
        "htmlLang": "pt-BR"
      }
    }
  },
  "markup": {
    "goldmark": {
      "renderer": {
        "unsafe": true
      }
    }
  },
  "params": {
    "description": "A blog to share my interests (mostly programming)",
    "email": "phelipe_teles@hotmail.com",
    "github": "https://github.com/phelipetls",
    "linkedin": "https://linkedin.com/in/phelipeteles",
    "resume": "https://phelipetls.github.io/resume/phelipe-teles-resume.pdf"
  },
  "taxonomies": {
    "tag": "tags"
  },
  "mediaTypes": {
     "text/markdown": {
        "suffixes": [
           "md"
        ]
     }
  }
}
